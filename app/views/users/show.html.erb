<h1> Specific user</h1>
<%= @user.email %>
<% if @user.image.attached? %>
    <%= image_tag @user.image, :width => 200, :height => 200 %>
<% end %>

<% if @user.images.exists? %>
    <h2> Images </h2>
    <% @user.images.each do |image| %>
        <%= image_tag image.photo, :width => 200, :height => 200 %>
    <% end %>
<% end %>
