<h1 class="mt-6">Users#index</h1>

<%= render(ButtonComponent.new(type: :primary)) do %>
    Submit
<% end %>

<%= search_form_for @q do |f| %>
  # Search if the name field contains...
  <%= f.label :email_cont %>
  <%= f.search_field :email_cont %>

  <%= f.submit %>
<% end %>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.email, user %>| <%= user.age %>
     
      <% if user.image.attached? %>
        <%= image_tag user.image, :width => 200, :height => 200 %>
      <% end %>
    </li>
  <% end %>
</ul>
