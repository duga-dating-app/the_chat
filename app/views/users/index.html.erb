<h1>Users#index</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.email %> | <%= user.age %>
       <% if user.image.attached? %>
        <div>
          <%= image_tag user.image, :alt => 'product_image', :width => 200, :height => 200 %>
        </div>
       <% end %>
    </li>
  <% end %>
</ul>
